class Board:
  field = []
  turn_number = 1

  def __init__(self):
    for i in range(0, 7):
        self.field.append([0, 0, 0, 0, 0, 0, 0])
        for j in range(0, 7):
            self.field[i][j] = '#'
    turn_number = 1

def printBoard(self):
        print("  1 2 3 4 5 6 7")
        for i in range(0, 7):
            print(i + " ", end="")
            for j in range(0, 7):
                print(self.field[i][j] + " ", end="")
            print("")

class ReflexAgent:
    id = ''

    def __init__(self, ID):
        id = ID

def main():
    b = Board()
    b.printBoard()



if __name__ == "__main__":
    main()
